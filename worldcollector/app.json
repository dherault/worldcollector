        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "a7f53e8c-52d9-4612-a290-e4a58a99231f"
      }
    },
    "owner": "dherault",
    "plugins": [
      "expo-router"
    ]
  }
}

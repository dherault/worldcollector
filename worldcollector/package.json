{
  "private": true,
  "name": "worldcollector",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "emulators": "firebase emulators:start --only auth,functions,firestore,storage,extensions --import=./emulator-data/current-export --export-on-exit=./emulator-data/current-export",
    "data:import": "firebase emulators:start --only auth,functions,firestore,storage,extensions` --import=./emulator-data/starter-export",
    "login": "firebase login --reauth",
    "port": "lsof -nP -iTCP -sTCP:LISTEN | grep 8080"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.0.1",
    "@expo/webpack-config": "^18.1.0",
    "@react-native-async-storage/async-storage": "1.17.11",
    "base-64": "^1.0.0",
    "email-validator": "^2.0.4",
    "expo": "~48.0.19",
    "expo-camera": "~13.2.1",
    "expo-constants": "~14.2.1",
    "expo-image-picker": "~14.1.1",
    "expo-linking": "~4.0.1",
    "expo-router": "^1.5.3",
    "expo-status-bar": "~1.4.4",
    "firebase": "^9.22.1",
    "nanoid": "^4.0.2",
    "native-base": "^3.4.28",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-get-random-values": "^1.9.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "~0.18.10",
    "react-navigation": "^4.4.4",
    "typescript": "^4.9.4",
    "expo-asset": "~8.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "eslint": "^8.42.0",
    "eslint-config-dherault-typescript": "^1.2.0"
  },
  "overrides": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0"
  }
}

{
  "private": true,
  "name": "worldcollector",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "emulators": "firebase emulators:start --only auth,functions,firestore,storage,extensions --import=./emulator-data/current-export --export-on-exit=./emulator-data/current-export",
    "data:import": "firebase emulators:start --only auth,functions,firestore,storage,extensions` --import=./emulator-data/starter-export",
    "login": "firebase login --reauth",
    "port": "lsof -nP -iTCP -sTCP:LISTEN | grep 8080",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.0.1",
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "algoliasearch": "^4.20.0",
    "base-64": "^1.0.0",
    "email-validator": "^2.0.4",
    "expo": "~49.0.16",
    "expo-asset": "~8.10.1",
    "expo-constants": "~14.4.2",
    "expo-image-picker": "~14.3.2",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.9",
    "expo-status-bar": "~1.6.0",
    "firebase": "^9.22.1",
    "nanoid": "^5.0.2",
    "native-base": "^3.4.28",
    "normalize-css-color": "^1.0.2",
    "react": "18.2.0",
    "react-instantsearch-core": "^7.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-get-random-values": "~1.9.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.9",
    "react-navigation": "^4.4.4",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/node": "^7.22.19",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "eslint": "^8.51.0",
    "eslint-config-dherault-typescript": "^1.3.2",
    "patch-package": "^8.0.0",
    "react-native-dotenv": "^3.4.9",
    "typescript": "^5.1.3"
  },
  "overrides": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0"
  }
}
